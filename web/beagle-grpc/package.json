    }
  ],
  "private": false,
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.14.0",
    "@zup-it/beagle-web": "^1.6.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
