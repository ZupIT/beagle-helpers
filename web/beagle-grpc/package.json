    "cross-blob": "^2.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-process": "^1.4.0",
    "mock-fs": "^4.13.0",
    "shebang-loader": "^0.0.1",
